public class Parabola {
    public static void main(String[] args) {
	int a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p;
	a=8;
	b=sq(a);
	c=7;
        d= sq(c);
	e=6;
        f= sq(e);
	g=5;
        h= sq(g);
	i=4;
        j= sq(i);
	k=3;
        l= sq(k);
	m=2;
        n= sq(m);
	o=1;
        p= sq(o);
	printGraph(b); 
	printGraph(d); 
	printGraph(f); 
	printGraph(h); 
	printGraph(j); 
	printGraph(l);
	printGraph(m); 
	printGraph(p);
        System.out.println("");
	printGraph(p);
	printGraph(m);
	printGraph(l);
	printGraph(i);
	printGraph(h);
	printGraph(f);
	printGraph(d);
	printGraph(b);
    } public static int sq(int n) {
    return n*n;
     }
    public static void printGraph(int x) {
	for (int i = 0; i < x; i++) {
	    System.out.print("*");
	}
	System.out.println("");
    }
}